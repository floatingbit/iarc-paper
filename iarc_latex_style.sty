% Title
\def\people{\begin{center}}
\def\endpeople{\end{center}}
\def\name#1{#1\\}
\def\org#1{\emph{#1}\\}
\def\maketitle{
	\newpage
	\null
	\vskip 1em
	\begin{center}
	\LARGE \bf \@title\\
	\end{center}
}

% Abstract
\def\abstract{\begin{center}
{\bf ABSTRACT\vspace{-0.0em}\vspace{0pt}}
\end{center}
\begin{center}
\begin{minipage}{5.5in}
}
\def\endabstract{\end{minipage}\end{center}}

% Style sections
%\setcounter{secnumdepth}{0}

\def\section{\@startsection {section}{1}{\z@}{-3.5ex plus -1ex minus 
    -.2ex}{0.1ex plus .2ex}{\bf\uppercase}}
\def\subsection{\@startsection{subsection}{2}{\z@}{-3.25ex plus -1ex minus 
   -.2ex}{0.1ex plus .2ex}{\bf}}
\def\subsubsection{\@startsection{subsubsection}{3}{\z@}{-3.25ex plus
-1ex minus -.2ex}{0.1ex plus .2ex}{\normalsize\em}}
\def\paragraph{\@startsection
     {paragraph}{4}{\z@}{3.25ex plus 1ex minus .2ex}{-1em}{\normalsize\bf}}
\def\subparagraph{\@startsection
     {subparagraph}{4}{\parindent}{3.25ex plus 1ex minus 
     .2ex}{-1em}{\normalsize\bf}}

